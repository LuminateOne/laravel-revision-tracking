{
    "name": "luminateone/revision-tracking",
    "description": "Tracking the changes of Models and restoring the changes for a Model",
    "type": "project",
    "authors": [
        {
            "name": "LuminateOne Limited",
            "email": "dev@luminate.one"
        }
    ],
    "autoload": {
        "psr-4": {
            "LuminateOne\\RevisionTracking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LuminateOne\\RevisionTracking\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LuminateOne\\RevisionTracking\\Providers\\RevisionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require": {},
    "require-dev": {
        "orchestra/testbench": "^3.0",
        "phpunit/phpunit": "^7.0"
    }
}
